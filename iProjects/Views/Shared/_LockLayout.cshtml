<!DOCTYPE html>
<html>

<head>
    <style>
        .form-css {
            display: block;
            padding: 15px;
            position: relative;
        }

        .navbar-ex1-collapse {
        }
    </style>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - iProjectManagement</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/toastr")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/clock.css" rel="stylesheet" />
    @RenderSection("head", required: false)
</head>
<body>


    @if (Request.IsAuthenticated)
    {
        <nav class="navbar" role="navigation">
            <div class="navbar navbar-inverse navbar-fixed-top">
                <div class="container">
                    <div class="navbar-header">
                        <button class="navbar-toggle" type="button" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand"><img src="~/Content/img/logo.png" /></a>
                        @Html.ActionLink("iManagement", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                    </div>
                    <div class="collapse navbar-collapse navbar-ex1-collapse">
                        <ul class="nav navbar-nav side">
                            
                        </ul>
                        @Html.Partial("_LoginPartial")
                    </div>

                </div>
            </div>
        </nav>
    }

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/toastr")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script src="@Url.Content("~/Scripts/js/plugins/jquery/jquery-ui.min.js")"></script>
    <script src="@Url.Content("~/Scripts/TimeOut.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/datatables/jquery.dataTables.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/jquery/jquery-migrate.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/jquery/globalize.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/bootstrap/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/uniform/jquery.uniform.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/mcustomscrollbar/jquery.mCustomScrollbar.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/ibutton/jquery.ibutton.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/select2/select2.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/tagsinput/jquery.tagsinput.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/jquery/jquery-ui-timepicker-addon.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/noty/jquery.noty.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/noty/layouts/topCenter.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/noty/layouts/topLeft.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/noty/layouts/topRight.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/noty/themes/default.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/fancybox/jquery.fancybox.pack.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/knob/jquery.knob.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/flot/jquery.flot.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/flot/jquery.flot.pie.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/flot/jquery.flot.resize.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/sparkline/jquery.sparkline.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/fullcalendar/fullcalendar.min.js")"></script>
    <script src="@Url.Content("https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false&libraries=places")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/shbrush/XRegExp.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/shbrush/shCore.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/shbrush/shBrushXml.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/shbrush/shBrushJScript.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/shbrush/shBrushCss.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/cleditor/jquery.cleditor.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/plupload/plupload.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/plupload/plupload.gears.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/plupload/plupload.silverlight.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/plupload/plupload.flash.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/plupload/plupload.browserplus.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/plupload/plupload.html4.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/plupload/plupload.html5.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/plupload/jquery.plupload.queue/jquery.plupload.queue.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/elfinder/elfinder.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/tinymce/tinymce.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins.js")"></script>
    <script src="@Url.Content("~/Scripts/js/actions.js")"></script>
    <script src="@Url.Content("~/Scripts/js/charts.js")"></script>
    <script src="@Url.Content("~/Scripts/js/maps.js")"></script>
    <script src="@Url.Content("~/Scripts/js/settings.js")"></script>
    <script src="@Url.Content("~/Content/Plugins/bootstrap-fileupload/bootstrap-fileupload.min.js")"></script>
    <script src="@Url.Content("~/Content/Plugins/select2/select2.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/plugins/platform.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.signalR-2.1.2.js")"></script>
    <script src="~/signalr/hubs"></script>
    <script src="@Url.Content("~/Scripts/bootstrap.js")"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/clock.js"></script>



</body>

</html>